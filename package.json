{
  "name": "personal-website-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "4.3.3",
    "@reach/router": "1.2.1",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.9.0",
    "react-reveal": "1.2.2",
    "react-scripts": "3.1.1",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build; docker build -t personal-website .;",
